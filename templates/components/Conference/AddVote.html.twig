<div{{ attributes }}>
    {{ form_start(form, {
        attr: {
            'data-action': 'live#action:prevent',
            'data-live-action-param': 'addVote'
        }
    }) }}
    {{ form_row(form.name) }}
    <button type="submit" class="btn btn-primary">{{ button_label|default('Add') }}</button>
    {{ form_end(form) }}
</div>
